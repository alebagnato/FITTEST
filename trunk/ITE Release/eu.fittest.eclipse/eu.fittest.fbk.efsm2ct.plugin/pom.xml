			<artifactId>eu.fittest.modelinference.uu2fbklogs</artifactId> <version>1.0.0-SNAPSHOT</version> 
			</dependency> <dependency> <groupId>eu.fittest.fbk</groupId> <artifactId>eu.fittest.fbk.cte2android</artifactId> 
			<version>1.0.0-SNAPSHOT</version> </dependency> <dependency> <groupId>eu.fittest.fbk</groupId> 
			<artifactId>eu.fittest.fsm2cte</artifactId> <version>1.0.2-SNAPSHOT</version> 
			</dependency> <dependency> <groupId>eu.fittest.fbk</groupId> <artifactId>eu.fittest.eventSequenceGenerator</artifactId> 
			<version>0.8.1-SNAPSHOT</version> </dependency> <dependency> <groupId>eu.fittest.fbk</groupId> 
			<artifactId>eu.fittest.xinputminer</artifactId> <version>0.0.1-SNAPSHOT</version> 
			</dependency -->
