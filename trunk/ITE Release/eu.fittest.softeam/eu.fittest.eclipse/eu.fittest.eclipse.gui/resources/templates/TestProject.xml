<?xml version="1.0" encoding="UTF-8"?>
<TestProject xmlns="http://www.fittest.eu/TestProject">
  <General>
    <type>FLASH</type>
    <baseURL>http://fittest.fbk.eu:8080</baseURL>
    <entryPage>index.html</entryPage>
    <serverFolder></serverFolder>
  </General>
  <Logging>
    <Instrumentation>
      <ghcrtOption></ghcrtOption>
    </Instrumentation>
    <LogTarget>
      <storeDir>Testing sessions</storeDir>
      <logLevel>2</logLevel>
    </LogTarget>
  </Logging>
  <ModelInference>
    <domainInputSpecFile>Models/xinput.xml</domainInputSpecFile>
    <absFuncDefFile>Models/absFunc.xml</absFuncDefFile>
    <modelFile>Models/model.fsm</modelFile>
    <inferenceTechnique>STATE-BASED</inferenceTechnique>
    <generateDot>true</generateDot>
    <gaParam>
      <populationSize>100</populationSize>
      <maxNumberOfGenerations>100</maxNumberOfGenerations>
      <mutationRate>0.1</mutationRate>
      <timeBudget>6000</timeBudget>
    </gaParam>
  </ModelInference>
  <TestGeneration>
    <cteFolder>Models/ctes</cteFolder>
    <modelVisitStrategy>VISITOR_BREADTHFIRST</modelVisitStrategy>
    <reduceTestSuite>true</reduceTestSuite>
    <sourcePackagePrefix>eu.fittest.test</sourcePackagePrefix>
    <seleniumDriverBrowser>ChromeDriver</seleniumDriverBrowser>
    <seleniumRemoteHost>localhost</seleniumRemoteHost>
    <seleniumRemotePort>4444</seleniumRemotePort>
    <seleniumBrowserConfig>*googlechrome</seleniumBrowserConfig>
    <gaParam>
      <populationSize>30</populationSize>
      <maxNumberOfGenerations>100</maxNumberOfGenerations>
      <mutationRate>0.1</mutationRate>
      <timeBudget>600</timeBudget>
    </gaParam>
  </TestGeneration>
 <Oracle>
    <GHCRTopts></GHCRTopts>
    <oracleFile>oracle.inv</oracleFile>
    <reportFile>report.txt</reportFile>
    <eventsToInclude></eventsToInclude>
    <fieldsToInclude></fieldsToInclude>
    <functionsToInclude></functionsToInclude>
    <lloOption></lloOption>
    <violationFile>violations.txt</violationFile>
  </Oracle>
</TestProject>
